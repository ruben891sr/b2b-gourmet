  // Contenido extra fuera de la documentacion

  // Buy Button
  "flex-layout.row#buy-button": {
    "props": {
      "blockClass": "colBuyButton",
      "marginTop": 4,
      "marginBottom": 7,
      "width": "grow"
    },
    "children": [
      "add-to-cart-button#comprarPDP"
    ]
  },
  "add-to-cart-button#comprarPDP": {
    "props":{
      "text": "Comprar"
    }
  },

