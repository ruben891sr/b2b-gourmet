    "props": {
      "width": "20%"
    },
    "children": [
      "sku-highlights#certificates-mobile"
    ]
  },
  "sku-highlights#certificates-mobile": {
    "props": {
      "conditional": {
        "highlight": "admin/editor.sku-list.highlights.chooseDefaultSpecification",
        "typeSpecifications": "Has Certificates"
      },
      "showLabel": true
    }
  },

