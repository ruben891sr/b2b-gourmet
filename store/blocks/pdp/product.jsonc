  "sku-quantity-selector#mobile": {
    "props": {
      "showLabel": true
    }
  },

  "flex-layout.row#item-main-mobile": {
    "props": {
      "preventHorizontalStretch": false,
      "blockClass": "rowItemMainDesktop"
    },
    "children": [
      // "flex-layout.col#sku-highlight-certificates",
